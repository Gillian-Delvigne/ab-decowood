{
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
  "customTemplates": [],
  "settings": {
    "appearanceTools": true,
    "color": {
      "gradients": [],
      "palette": [
        { "slug": "theme-0", "name": "Primary", "color": "#000000" },
        { "slug": "theme-1", "name": "Secondary", "color": "#7c4911" },
        { "slug": "theme-2", "name": "Text", "color": "#7a7a7a" },
        { "slug": "theme-3", "name": "Accent", "color": "#9e6726" },
        { "slug": "theme-4", "name": "Brown", "color": "#ce996a" },
        { "slug": "theme-6", "name": "Light Brown", "color": "#a26e35" },
        { "slug": "theme-7", "name": "Dark Brown", "color": "#42270a" },
        { "slug": "theme-5", "name": "White", "color": "#ffffff" }
      ]
    },
    "custom": {
      "headings": {
        "typography": {
          "fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
          "slug": "system-fonts",
          "name": "System fonts"
        }
      }
    },
    "layout": { "contentSize": "1140px", "wideSize": "1200px" },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true,
      "units": ["px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "customFontSize": true,
      "fontFamilies": [],
      "fontSizes": [
        {
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          },
          "size": "1rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          },
          "size": "1.125rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.75rem",
            "max": "1.875rem"
          },
          "size": "1.75rem",
          "slug": "large"
        },
        {
          "fluid": false,
          "size": "2.25rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "4rem",
            "max": "10rem"
          },
          "size": "10rem",
          "slug": "xx-large"
        }
      ]
    }
  },
  "styles": { 
	"elements": {
		"h1": {
			"typography": {
				"fontSize":"x-large"
			}
		}
	},
	"typography": {"fontFamily": "var(--wp-preset--font-family--system-fonts)"}
   },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 2
}
